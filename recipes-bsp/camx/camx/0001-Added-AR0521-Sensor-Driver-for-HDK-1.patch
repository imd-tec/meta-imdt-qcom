index 00000000..bad365f2
--- /dev/null
+++ b/chi-cdk/oem/qcom/sensor/ar0521/ar0521.xml
@@ -0,0 +1,4713 @@
+<?xml version="1.0" encoding="UTF-8"?>
+<!--========================================================================-->
+<!-- Copyright (c) 2023 IMD Technologies                                    -->
+<!-- All Rights Reserved.                                                   -->
+<!-- Confidential and Proprietary - IMD Technologies                        -->
+<!--========================================================================-->
+<sensorDriverData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../api/sensor/camxsensordriver.xsd">
+  <module_version major_revision="1" minor_revision="0" incr_revision="0" />
+  <slaveInfo>
+    <!--Name of the sensor -->
+    <sensorName>ar0521</sensorName>
+    <!--8-bit or 10-bit WRITE slave address -->
+    <slaveAddress>0x6C</slaveAddress>
+    <!--Register address / data size in bytes -->
+    <regAddrType range="[1,4]">2</regAddrType>
+    <!--Register address / data size in bytes -->
+    <regDataType range="[1,4]">2</regDataType>
+    <!--Register address for sensor Id -->
+    <sensorIdRegAddr>0</sensorIdRegAddr>
+    <!--Sensor Id -->
+    <sensorId>0x0457</sensorId>
+    <!--Mask for sensor id. Sensor Id may only be few bits -->
+    <sensorIdMask>4294967295</sensorIdMask>
+    <!--I2C frequency mode of slave
+        Supported modes are: STANDARD (100 KHz), FAST (400 KHz), FAST_PLUS (1 MHz), CUSTOM (Custom frequency in DTSI)
+        AR0521 can run at all supported modes but EXTCLK must atleast 6 times faster than I2C SCLK -->
+    <i2cFrequencyMode>FAST</i2cFrequencyMode>
+    <!--Sequence of power configuration type and configuration value required to control power to the device -->
+    <powerUpSequence>
+      <!--Power setting configuration
+          Contains: configType, configValue and delay in milli seconds -->
+      <powerSetting>
+        <!--Power configuration type
+            Supported types are: MCLK, VANA, VDIG, VIO, VAF, RESET, STANDBY -->
+        <configType>RESET</configType>
+        <!--Configuration value for the type of configuration -->
+        <configValue>0</configValue>
+        <!--Delay in milli seconds -->
+        <delayMs>1</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>VIO</configType>
+        <configValue>0</configValue>
+        <delayMs>10</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>VDIG</configType>
+        <configValue>0</configValue>
+        <delayMs>10</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>VANA</configType>
+        <configValue>0</configValue>
+        <delayMs>10</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>MCLK</configType>
+        <!-- Datasheet specifies the need to wait for 1-500ms + 45000 EXTCLKs before any I2C Operations
+             CALCULATION 45000 EXTCLKs => 45000 * (1 / MCLK_FREQ) = 2.344ms @ 19.2MHz EXT CLK 
+             Therefore minimum delay = 1ms + 2.344ms = 3.344ms 
+             Also using the reset time as wait time-->
+        <configValue>19200000</configValue>
+        <delayMs>1</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>RESET</configType>
+        <configValue>1</configValue>
+        <delayMs>10</delayMs>
+      </powerSetting>
+    </powerUpSequence>
+    <!--Sequence of power configuration type and configuration value required to control power to the device -->
+    <powerDownSequence>
+      <!--Power setting configuration: Contains: configType, configValue and delay in milli seconds -->
+      <powerSetting>
+        <!--Power configuration type: Supported types are: MCLK, VANA, VDIG, VIO, VAF, RESET, STANDBY -->
+        <configType>MCLK</configType>
+        <!--Configuration value for the type of configuration -->
+        <configValue>0</configValue>
+        <delayMs>0</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>RESET</configType>
+        <configValue>0</configValue>
+        <delayMs>15</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>VANA</configType>
+        <configValue>0</configValue>
+        <delayMs>15</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>VDIG</configType>
+        <configValue>0</configValue>
+        <delayMs>15</delayMs>
+      </powerSetting>
+      <powerSetting>
+        <configType>VIO</configType>
+        <configValue>0</configValue>
+        <delayMs>15</delayMs>
+      </powerSetting>
+    </powerDownSequence>
+  </slaveInfo>
+  <!--Addresses for various sensor registers -->
+  <regAddrInfo>
+    <!--Register address to program width  - x_output_size (R/W) [0, 4095] -->
+    <xOutput>0x034C</xOutput>
+    <!--Register address to program height - y_output_size (R/W) [0, 4095] -->
+    <yOutput>0x034E</yOutput>
+    <!--Register address to program Frame length lines -->
+    <frameLengthLines>0x0340</frameLengthLines>
+    <!--Register address to program Line length pixel clock -->
+    <lineLengthPixelClock>0x0342</lineLengthPixelClock>
+    <!--Register address to program coarse integration time - coarse_integration_time_ (R/W)  [0, 65535] -->
+    <coarseIntgTimeAddr>0x3012</coarseIntgTimeAddr>
+    <!--Register address to program analog global gain channel - global_gain (R/W) -->
+    <globalGainAddr>0x305E</globalGainAddr>
+    <!--Register address to program digital gain for Red channel
+        This address is optional and must be provided if supported by sensor - digital_gain_red (R/W)  [0,2047] -->
+    <digitalGainRedAddr>0x0210</digitalGainRedAddr>
+    <!--Register address to program digital gain for green red channel
+        This address is optional and must be provided if supported by sensor - digital_gain_greenr (R/W) [0,2047] -->
+    <digitalGainGreenRedAddr>0x020E</digitalGainGreenRedAddr>
+    <!--Register address to program digital gain for blue channel
+        This address is optional and must be provided if supported by sensor - digital_gain_blue (R/W) [0,2047] -->
+    <digitalGainBlueAddr>0x0212</digitalGainBlueAddr>
+    <!--Register address to program digital gain for green blue channel
+        This address is optional and must be provided if supported by sensor - digital_gain_greenb (R/W) [0,2047] -->
+    <digitalGainGreenBlueAddr>0x0214</digitalGainGreenBlueAddr>
+    <!--Register address to program manual test pattern value for Red channel - test_data_red (R/W) [0, 1023] -->
+    <testPatternRAddr>0x0602</testPatternRAddr>
+    <!--Register address to program manual test pattern value for Green Red channel - test_data_greenr (R/W) [0, 1023] -->
+    <testPatternGRAddr>0x0604</testPatternGRAddr>
+    <!--Register address to program manual test pattern value for Blue channel - test_data_blue (R/W) [0, 1023] -->
+    <testPatternBAddr>0x0606</testPatternBAddr>
+    <!--Register address to program manual test pattern value for Green Blue channel - test_data_greenb (R/W) [0, 1023] -->
+    <testPatternGBAddr>0x0608</testPatternGBAddr>
+  </regAddrInfo>
+  <!--Information of all the resolutions
+      Number of resolutions is detected from number of resolutionData nodes -->
+  <resolutionInfo>
+    <sensorVersion>0</sensorVersion>
+    <!--res0 - MIPI 2 Lane | 2600 (H) x 1952 (V) | 10 FPS | RAW 10 BIT | Non Continuous Mode!-->
+    <resolutionData>
+      <!--List of seamless mode switch usecases that this sensor mode can support -->
+      <transitionGroups>NONE</transitionGroups>
+      <!--Color filter arrangement of the sensor
+          Supported filter arrangements are: BAYER_BGGR, BAYER_GBRG, BAYER_GRBG, BAYER_RGGB, BAYER_Y, YUV_UYVY, YUV_YUYV -->
+      <colorFilterArrangement>BAYER_GRBG</colorFilterArrangement>
+      <!--Stream information -->
+      <streamInfo>
+        <!--Information for a stream data
+            element for vc
+            element for dt
+            element for dimension
+            element for bitWidth
+            element for type -->
+        <streamConfiguration>
+          <!--Virtual Channel of the data
+              Valid values for virtual channel are: 0, 1, 2 and 3 -->
+          <vc range="[0,3]">0</vc>
+          <!--Data type of the stream. Default value is 0x2B (10-bit RAW) -->
+          <dt>0x2B</dt>
+          <!--Frame dimension: contains xStart, yStart, width and height -->
+          <frameDimension>
+            <xStart>0</xStart>
+            <yStart>0</yStart>
+            <width>2600</width>
+            <height>1952</height>
+          </frameDimension>
+          <!--Bit width of the data -->
+          <bitWidth>10</bitWidth>
+          <!--Type of the stream
+              Supported stream types are: BLOB, IMAGE, PDAF, HDR, META, IMAGE_SHORT, IMAGE_MIDDLE -->
+          <type>IMAGE</type>
+        </streamConfiguration>
+      </streamInfo>
+      <!--Line length pixel clock of frame
+          Typically this value is the active width + blanking width -->
+      <!-- For the AR0521 this lineLengthPixelClock = LINE_LENGTH_PCK_REG_VALUE / 2 -->
+      <lineLengthPixelClock>2830</lineLengthPixelClock>
+      <!--Frame length lines of frame
+          Typically this value is the active height + blanking height -->
+      <frameLengthLines>5200</frameLengthLines>
+      <!--Minimum horizontal blanking interval in pixels -->
+      <minHorizontalBlanking>230</minHorizontalBlanking>
+      <!--Minimum vertical blanking interval in lines -->
+      <minVerticalBlanking>316</minVerticalBlanking>
+      <!--Output pixel clock -->
+      <!-- MIPI DATA RATE (per lane) = WORLD_CLOCK * BIT DEPTH = 73.6MHz * 10 = 736MHz-->
+      <!-- Qualcomm Output Pixel Clock = MIPI Data Rate * Lane Count / Bit Depth = 736MHz * 2 / 10 = 147.2MHz-->   
+      <outputPixelClock>147200000</outputPixelClock>
+      <!--Horizontal binning value -->
+      <horizontalBinning>1</horizontalBinning>
+      <!--Vertical binning value -->
+      <verticalBinning>1</verticalBinning>
+      <!--Maximum frame rate -->
+      <frameRate>10.0</frameRate>
+      <!--Number of lanes in which the data is streamed -->
+      <laneCount>2</laneCount>
+      <downScaleFactor>1.0</downScaleFactor>
+      <!--Settle time in nano seconds -->
+      <!-- Settle Time = 85ns + 6UI / TIMER_CLOCK_PERIOD_NS 
+          UI = Unit Interval            = MIPI Clock Period (per lane) in ns = 1 / 736MHz = 1.36ns
+          Timer Clock Period for SM8550 = 1 / 400MHz = 2.5ns 
+          Settle Time                   = ((85ns + 6 * (1.36ns)) / 2.5ns) - 10  
+                                        = 27.26ns (Roughly 28ns) -->      
+      <settleTimeNs>28</settleTimeNs>
+      <!--Flag to know if the sensor is a 3phase sensor -->
+      <is3Phase>0</is3Phase>
+      <!--Sequence of register settings to configure the device -->
+      <resSettings>
+        <regSetting>
+          <!--IMDT CUSTOM CONFIG - GLOBAL_GAIN: Digital Gain = 1.0, Analog Gain = 8.0  -->
+          <registerAddr>0x305E</registerAddr>
+          <registerData>0x2070</registerData>
+          <regAddrType range="[1,4]">2</regAddrType>
+          <regDataType range="[1,4]">2</regDataType>
+          <operation>WRITE</operation>
+          <delayUs>0</delayUs>
+        </regSetting>
+        <regSetting>
+          <!--IMDT CUSTOM CONFIG - Normal Stream -->
+          <registerAddr>0x0600</registerAddr>
+          <registerData>0x0000</registerData>
+          <regAddrType range="[1,4]">2</regAddrType>
+          <regDataType range="[1,4]">2</regDataType>
+          <operation>WRITE</operation>
+          <delayUs>0</delayUs>
+        </regSetting>
+        <!--[PLL_settings]-->
+        <regSetting>
+          <!--VT_PIX_CLK_DIV = 5-->
+          <registerAddr>0x0300</registerAddr>
+          <registerData>0x0005</registerData>
+          <regAddrType range="[1,4]">2</regAddrType>
+          <regDataType range="[1,4]">2</regDataType>
+          <operation>WRITE</operation>
+          <delayUs>0</delayUs>
+        </regSetting>
+        <regSetting>
+          <!--VT_SYS_CLK_DIV = 1-->
+          <registerAddr>0x0302</registerAddr>
+          <registerData>0x0001</registerData>
+          <regAddrType range="[1,4]">2</regAddrType>
+          <regDataType range="[1,4]">2</regDataType>
+          <operation>WRITE</operation>
+          <delayUs>0</delayUs>
+        </regSetting>
+        <regSetting>
+          <!--PRE_PLL_CLK_DIV, bits 0x003F = 3, PRE_PLL_CLK_DIV2, bits 0x3F00 = 3-->
