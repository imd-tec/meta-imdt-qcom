+project (camx)
+
+# Macros for camx
+set (CAMX_PATH ${CMAKE_CURRENT_LIST_DIR}/../../../../../../../camx)
+set (CAMX_OUT_PATH ${PROJECT_BINARY_DIR})
+set (CAMX_LIB_OUTPUT_PATH camera/components)
+set (TARGET_BOARD_PLATFORM "kalama")
+
+message(STATUS "CAMX_PATH = ${CAMX_PATH}")
+message(STATUS "PROJECT_OUT_PATH = ${PROJECT_BINARY_DIR}")
+
+# Macros for camx-common
+set (CAMX_COMMON_PATH ${CAMX_PATH}/../camx-common)
+set (CAMX_COMMON_OUT_PATH ${PROJECT_BINARY_DIR})
+
+# Macros for chi-cdk
+set (CAMX_CHICDK_PATH ${CAMX_PATH}/../chi-cdk)
+set (CAMX_CHICDK_OUT_PATH ${PROJECT_BINARY_DIR})
+set (CAMX_CHICDK_API_PATH ${CAMX_CHICDK_PATH}/api)
+set (CAMX_CHICDK_CORE_PATH ${CAMX_CHICDK_PATH}/core)
+set (CAMX_CHICDK_OEM_PATH ${CAMX_CHICDK_PATH}/oem)
+set (CAMX_CHICDK_TOOLS_PATH ${CAMX_CHICDK_PATH}/tools)
+set (CAMX_CHICDK_TOPOLOGY_PATH ${CAMX_CHICDK_PATH}/oem/topology/titan)
+set (CAMX_CHICDK_TEST_PATH ${CAMX_CHICDK_PATH}/test)
+set (CAMX_EXT_PATH ${CAMX_PATH}/../ext)
+set (CAMX_API_PATH ${CAMX_PATH}/../camx-api)
+
+# Macros for ANDROID API version (Android 10/Q=29 11/R=30, 12/S=31)
