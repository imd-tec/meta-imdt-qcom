+if(${ANDROID_API} EQUAL 31)
+    add_definitions (-DCAMX_ANDROID_API=31)
+else()
+    add_definitions (-DCAMX_ANDROID_API=30)
+endif()
+
+# Individual makefiles for libs and bins under chi-cdk project
+include ("${CAMX_CHICDK_PATH}/oem/qcom/buildbins/build/linuxembedded/binary_${TARGET_BOARD_PLATFORM}/com.qti.sensormodule.imdt_ar0521.cmake")
